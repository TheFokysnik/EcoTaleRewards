$C = "../Common.ui";

@GoldTitle = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 18,
  TextColor: #ffaa00,
  RenderBold: true,
  HorizontalAlignment: Center
);

@White = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 13,
  TextColor: #ffffff
);

@Gray = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 12,
  TextColor: #aaaaaa
);

@Value = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 13,
  TextColor: #55ff55,
  RenderBold: true
);

@SectionLabel = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 14,
  TextColor: #ffaa00,
  RenderBold: true
);

@Error = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 13,
  TextColor: #ff4444,
  RenderBold: true
);

@Success = LabelStyle(
  ...$C.@DefaultLabelStyle,
  FontSize: 13,
  TextColor: #55ff55,
  RenderBold: true
);

