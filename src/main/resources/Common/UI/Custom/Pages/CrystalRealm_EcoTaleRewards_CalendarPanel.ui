// =====================================================
//  BUTTON STYLES
// =====================================================

@ClaimBtnLabelStyle = LabelStyle(
  FontSize: 11,
  RenderBold: true,
  TextColor: #eeffee,
  HorizontalAlignment: Center,
  VerticalAlignment: Center
);

@ClaimBtnStyle = TextButtonStyle(
  Default: (Background: #2a7a4a, LabelStyle: @ClaimBtnLabelStyle),
  Hovered: (Background: #35a060, LabelStyle: @ClaimBtnLabelStyle),
  Pressed: (Background: #226640, LabelStyle: @ClaimBtnLabelStyle),
  Disabled: (Background: #2a3545, LabelStyle: (...@ClaimBtnLabelStyle, TextColor: #667788)),
  Sounds: $C.@ButtonSounds
);

@ReturnBtnLabelStyle = LabelStyle(
  FontSize: 12,
  RenderBold: true,
  TextColor: #fff5dd,
  HorizontalAlignment: Center,
  VerticalAlignment: Center
);

@ReturnBtnStyle = TextButtonStyle(
  Default: (Background: #8a6520, LabelStyle: @ReturnBtnLabelStyle),
  Hovered: (Background: #aa7d28, LabelStyle: @ReturnBtnLabelStyle),
  Pressed: (Background: #705218, LabelStyle: @ReturnBtnLabelStyle),
  Disabled: (Background: #3a3520, LabelStyle: (...@ReturnBtnLabelStyle, TextColor: #998866)),
  Sounds: $C.@ButtonSounds
);

// =====================================================
//  MAIN LAYOUT
// =====================================================

$C.@PageOverlay {
  LayoutMode: Middle;

  $C.@DecoratedContainer {
    Anchor: (Width: 1200, Height: 850);

    #Title {
      Background: (Color: #0a1020(0.9));
      Label #TitleLabel { Style: @GoldTitle; Text: "Daily Rewards"; }
    }

    #Content {
      LayoutMode: Top;
      Padding: (Left: 16, Right: 16, Top: 10, Bottom: 10);
      Background: (Color: #080e1a);

      // -- Banners --
      Group #ErrorBanner {
        Anchor: (Height: 30); Visible: false;
        Background: #3a1515(0.9);
        Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);
        Label #ErrorText { Style: @Error; Text: ""; }
      }
      Group #SuccessBanner {
        Anchor: (Height: 30); Visible: false;
        Background: #153a1a(0.9);
        Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);
        Label #SuccessText { Style: @Success; Text: ""; }
      }

      // -- Stats Card (top summary) --
      Group #StatsCard {
        Anchor: (Height: 72);
        LayoutMode: Left;
        Background: (Color: #111a2e(0.9));
        Padding: (Left: 20, Right: 20, Top: 10, Bottom: 10);

        Group #StreakSection {
          LayoutMode: Top;
          Anchor: (Width: 200);
          Group {
            LayoutMode: Left;
            Anchor: (Height: 22);
            Label #StreakLabel { Style: @ProgressLabel; Anchor: (Width: 80); Text: ""; }
            Label #StreakValue { Style: @Streak; FlexWeight: 1; Text: ""; }
          }
          Group {
            LayoutMode: Left;
            Anchor: (Height: 22);
            Label #MultLabel { Style: @ProgressLabel; Anchor: (Width: 80); Text: ""; }
            Label #MultValue { Style: @Value; FlexWeight: 1; Text: ""; }
          }
        }

        Group {
          FlexWeight: 1;
          LayoutMode: Top;
          Padding: (Left: 24, Right: 24, Top: 4);
          Label #ProgressText {
            Style: (...$C.@DefaultLabelStyle, FontSize: 13, TextColor: #c0ccdd, HorizontalAlignment: Center);
            Anchor: (Height: 18);
            Text: "";
          }
          Label #NextMsLabel {
            Style: (...$C.@DefaultLabelStyle, FontSize: 11, TextColor: #7a8aa0, HorizontalAlignment: Center);
            Anchor: (Height: 16);
            Text: "";
          }
        }

        Group #TotalSection {
          LayoutMode: Top;
          Anchor: (Width: 200);
          Group {
            LayoutMode: Left;
            Anchor: (Height: 22);
            Label #TotalCoinsLabel { Style: @ProgressLabel; Anchor: (Width: 90); Text: ""; }
            Label #TotalCoinsValue { Style: @CoinLabel; FlexWeight: 1; Text: ""; }
          }
          Group {
            LayoutMode: Left;
            Anchor: (Height: 22);
            Label #TotalXpLabel { Style: @ProgressLabel; Anchor: (Width: 90); Text: ""; }
            Label #TotalXpValue { Style: @XpLabel; FlexWeight: 1; Text: ""; }
          }
        }
      }

      Group { Anchor: (Height: 6); }

      // -- Return Banner --
      Group #ReturnBanner {
        Anchor: (Height: 42); Visible: false;
        LayoutMode: Left;
        Background: #1a1508(0.9);
        Padding: (Left: 16, Right: 12, Top: 6, Bottom: 6);
        Label #ReturnText { Style: @Gold; FlexWeight: 1; Text: ""; }
        TextButton #ReturnClaimBtn {
          Anchor: (Width: 160, Height: 30);
          Text: "Claim";
          Style: @ReturnBtnStyle;
        }
      }

      Group { Anchor: (Height: 4); }
      Label #SecCalendar { Style: @SectionLabel; Anchor: (Height: 18); Text: "Calendar"; }
      Group { Anchor: (Height: 4); }

      // =====================================================
      //  CALENDAR GRID (30 days, 6 rows x 5 cols)
      // =====================================================
