            Group { LayoutMode: Left; Anchor: (Height: 16); Label #Day30Coins { Style: @CoinLabel; Anchor: (Width: 90); Text: ""; } Label #Day30Xp { Style: @XpLabel; FlexWeight: 1; Text: ""; } }
            Group #Day30Item1 { Anchor: (Height: 22); Visible: false; LayoutMode: Left; Padding: (Top: 1); ItemIcon #Day30Icon1 { Anchor: (Width: 20, Height: 20); } Group { Anchor: (Width: 3); } Label #Day30Item1Name { Style: @ItemLabel; FlexWeight: 1; Text: ""; } }
            Group #Day30Item2 { Anchor: (Height: 22); Visible: false; LayoutMode: Left; Padding: (Top: 1); ItemIcon #Day30Icon2 { Anchor: (Width: 20, Height: 20); } Group { Anchor: (Width: 3); } Label #Day30Item2Name { Style: @ItemLabel; FlexWeight: 1; Text: ""; } }
            Group { FlexWeight: 1; }
            TextButton #Day30Btn { Anchor: (Height: 24); Text: ""; Visible: false; Style: @ClaimBtnStyle; }
          }
        }
      }

      // -- Footer info --
      Group { Anchor: (Height: 4); }
      Group #ClaimArea {
        Anchor: (Height: 28);
        Padding: (Left: 8, Right: 8, Top: 4, Bottom: 4);
        Background: #111a2e(0.7);
        Label #ClaimDayLabel { Style: @White; FlexWeight: 1; Text: ""; }
      }
    }
  }
}

