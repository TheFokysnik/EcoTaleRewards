              Label #Day1Coins { Style: @CoinLabel; Anchor: (Width: 90); Text: ""; }
              Label #Day1Xp { Style: @XpLabel; FlexWeight: 1; Text: ""; }
            }
            Group #Day1Item1 { Anchor: (Height: 22); Visible: false; LayoutMode: Left; Padding: (Top: 1);
              ItemIcon #Day1Icon1 { Anchor: (Width: 20, Height: 20); }
